{
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-expressions": 0,
    "no-unused-vars": 0,
    "no-console": 2,
    "func-names": 0,
    "import": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "no-undef": 0,
    "no-new": 0,
    "import/first": 0,
    "no-useless-rename": 0,
    "semi": 0,
    "no-trailing-spaces": 0,
  }
}